{
  "baseUrl": "#{EZPAY_API_URL}#",
  "chromeWebSecurity": false,
  "defaultCommandTimeout": 10000,
  "env": {
    "username": "#{EZPAY_TOKEN_USER}#",
    "password": "#{EZPAY_TOKEN_PASS}#",
    "tokenUrl": "#{EZPAY_TOKEN_URL}#",     
    "api-svc-url": "#{EZPAY_API_URL}#",
    "authorization": "bearer --BEARER_TOKEN---",
    "email-of-bearer-token-user": "#{EMAIL_OF_BEARER_TOKEN_USER}#",
    "x-aptean-apim-checkout-consumer": "#{X_APTEAN_APIM_CHECKOUT_CONSUMER}#",
    "x-aptean-apim": "#{X_APTEAN_APIM}#",
    "x-aptean-tenant": "#{X_APTEAN_TENANT}#",
    "x-aptean-tenant-secret": "#{X_APTEAN_TENANT_SECRET}#",
    "x-aptean-account": "#{X_APTEAN_ACCOUNT}#",
    "x-aptean-product": "#{X_APTEAN_PRODUCT}#",    
    "wepay-base-url": "#{WEPAY_API_URL}#",
    "wepay-app-id": "#{WEPAY_APP_ID}#",
    "wepay-app-secret": "#{WEPAY_APP_SECRET}#",
    "wepay-api-version": "#{WEPAY_API_VERSION}#",
    "wepay-account-id": "#{WEPAY_ACCOUNT_ID}#",
    "currency": "#{CURRENCY}#",
    "runtimeEnv": "local",
    "storageAccountConnString": "#{STORAGE_ACCOUNT_CONN_STRING}#"
  },
  "supportFile": "cypress/support/index.ts",
  "reporter": "cypress-mochawesome-reporter",
  "reporterOptions": {
    "reportDir": "/e2e/cypress/report",
    "charts": true,
    "reportPageTitle": "ezpay-api-svc",
    "embeddedScreenshots": false
  },
  "video": false,
  "screenshotsFolder": "/e2e/cypress/screenshots"  
}